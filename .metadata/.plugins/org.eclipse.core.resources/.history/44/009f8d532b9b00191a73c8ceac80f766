package Message;

public class FileTransferMessage extends Message{
	
	int size;
	String filePath;
	byte[] buff;
	
	FileTransferMessage(long _size, String _filePath, byte[] _buff){
		size = _size;
		filePath = _filePath;
		buff = _buff;
	}
	
	@Override
	public Message handle() {
		return null;
	}

}
